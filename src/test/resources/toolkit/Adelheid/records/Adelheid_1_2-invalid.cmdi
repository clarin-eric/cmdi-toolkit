                    <cmdp:Organisation>Radboud University Nijmegen</cmdp:Organisation>
                    <cmdp:Telephone>+31 - 24 361 2836</cmdp:Telephone>
                </cmdp:Contact>
                <cmdp:Duration>
                    <cmdp:StartYear>2010</cmdp:StartYear>
                    <cmdp:CompletionYear>2012</cmdp:CompletionYear>
                </cmdp:Duration>
            </cmdp:Project>
