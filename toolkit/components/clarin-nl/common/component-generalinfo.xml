<?xml version="1.0" encoding="UTF-8"?>
<CMD_ComponentSpec xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.clarin.eu/cmd http://www.clarin.eu/cmd/general-component-schema.xsd"
                   isProfile="false">
    <Header/>
    <CMD_Component name="GeneralInfo">
        <CMD_Element name="Name" ValueScheme="string" CardinalityMin="1" CardinalityMax="unbounded"
                   ConceptLink="http://www.isocat.org/datcat/DC-2544"/>
        <CMD_Element name="Title" ValueScheme="string"
                   ConceptLink="http://www.isocat.org/datcat/DC-2545"/>
        <CMD_Element name="ID" ValueScheme="string" CardinalityMax="unbounded"
                   ConceptLink="http://www.isocat.org/datcat/DC-2573"/>
        <CMD_Element name="Version" ValueScheme="string"
                   ConceptLink="http://www.isocat.org/datcat/DC-2547"/>
        
        <!-- vergelijken met DC! -->
        <CMD_Element name="TimeCoverage" ValueScheme="string"
                   ConceptLink="http://www.isocat.org/datcat/DC-2502"/>
        <CMD_Element name="Description" ValueScheme="string" CardinalityMin="1" CardinalityMax="1"
                   ConceptLink="http://www.isocat.org/datcat/DC-2520"/>
        <CMD_Element name="PublicationYear" CardinalityMin="0" CardinalityMax="1"
                   ConceptLink="http://www.isocat.org/datcat/DC-2538"
                   ValueScheme="gYear"/>
