                <cmdp:Operations>
                    <cmdp:Operation>
                        <cmdp:Name>createProject</cmdp:Name>
                        <cmdp:Description>This is necessary before attempting to upload any files; it initialises an empty new Adelheid project.</cmdp:Description>
                        <cmdp:Input>
                            <cmdp:Parameter>
                                <cmdp:Name>project</cmdp:Name>
                                <cmdp:Description>The name to be used for the new Adelheid project. Do not use spaces or special characters in the project name, only underscores and alphanumeric characters are allowed</cmdp:Description>
