                                <cmdp:TechnicalMetadata>
                                    <cmdp:CharacterEncoding>UTF-8</cmdp:CharacterEncoding>
                                    <cmdp:ContentEncoding>
                                        <!--<URL><!-\- TODO: URL pointing to the schema of CLAM XML -\-></URL>-->
                                        <cmdp:ResourceFormat>CLAM XML</cmdp:ResourceFormat>
