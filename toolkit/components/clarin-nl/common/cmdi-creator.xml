<?xml version="1.0" encoding="UTF-8"?>
<CMD_ComponentSpec xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.clarin.eu/cmd http://www.clarin.eu/cmd/general-component-schema.xsd" isProfile="false">
    
    <Header>
        <Name>Creator</Name>
        <Description>Component contains (contact) information about a person and/or his/her organisation that was involved in the creation process of a resource</Description>
    </Header>
    
    <CMD_Component name="Creator">
        
        <CMD_Element name="Role" ConceptLink="http://www.isocat.org/datcat/DC-2513" CardinalityMin="0" CardinalityMax="unbounded" ValueScheme="string"/>

        <!-- within the component Creator the component Contact contains contact information for a person or an organisation that was involved in the creation process of a resource -->
        <CMD_Component filename="http://www.clarin.eu/cmd/components/clarin-nl/common/cmdi-contact.xml" CardinalityMin="1" CardinalityMax="unbounded"/>

    </CMD_Component>    
</CMD_ComponentSpec>