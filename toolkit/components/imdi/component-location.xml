<?xml version="1.0" encoding="UTF-8"?>
<CMD_ComponentSpec xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../general-component-schema.xsd" xmlns:xi="http://www.w3.org/2001/XInclude">
    
    <Header/>
    
    <CMD_Component name="Location" CardinalityMin="1" CardinalityMax="1">
        
        <CMD_Element name="Continent">
            <ValueScheme>      
                <enumeration>
                    <item>Unknown</item>
                    <item>Unspecified</item>
                    <item>Africa</item>
                    <item>Asia</item>
                    <item>Europe</item>
                    <item>Australia</item>
                    <item>Oceania</item>
                    <item>North-America</item>
                    <item>Middle-America</item>
                    <item>South-America</item>
                </enumeration>
            </ValueScheme>
        </CMD_Element>
        <CMD_Element name="Country" ValueScheme="string"/>
        <CMD_Element name="Region" ValueScheme="string" CardinalityMin="0" CardinalityMax="unbounded"/>
        <CMD_Element name="Address" ValueScheme="string" CardinalityMin="0"/>
        
    </CMD_Component>
    
</CMD_ComponentSpec>
