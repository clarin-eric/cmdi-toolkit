<?xml version="1.0" encoding="UTF-8"?>
<CMD_ComponentSpec xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.clarin.eu/cmd http://www.clarin.eu/cmd/general-component-schema.xsd"
                   isProfile="false">
    <Header/>
    <CMD_Component name="LexiconType">
        <CMD_Element name="LexiconType" ConceptLink="http://www.isocat.org/rest/dc/2487">
            <ValueScheme>
                <enumeration>
                    <item>word list</item>
                    <item>dictionary</item>
                </enumeration>
            </ValueScheme>
        </CMD_Element>
      <CMD_Component name="LexiconSubtype">
        <CMD_Element name="Linguality">
            <ValueScheme>
                <enumeration>
                    <item>monolingual</item>
                    <item>bilingual</item>
                    <item>multilingual</item>
                    <item>NAP</item>
                </enumeration>
            </ValueScheme>
        </CMD_Element>
        <CMD_Element name="HeadWordType" ConceptLink="http://www.isocat.org/rest/dc/2486">
            <ValueScheme>
                <enumeration>
                    <item>lemma</item>
                    <item>word form</item>
                    <item>sentence</item>
                    <item>phrase</item>
                    <item>multi-word expression</item>
                    <item>other</item>
                </enumeration>
            </ValueScheme>
        </CMD_Element>
      </CMD_Component>
    </CMD_Component>
</CMD_ComponentSpec>