                <DocumentationLanguages>
                    <Description>
                        <Description>Not all documentation is available in both languages.</Description>
                    </Description>
                    <Language>
                        <LanguageName>English</LanguageName>
                        <ISO639>
                            <iso-639-3-code>eng</iso-639-3-code>
                        </ISO639>                  
                    </Language>
                    <Language>
                        <LanguageName>Dutch</LanguageName>
                        <ISO639>
                            <iso-639-3-code>dut</iso-639-3-code>
                        </ISO639>
                    </Language>  
                </DocumentationLanguages>
                <Access>
                    <Availability>free for academic parties; available to commercial parties, but not for free; user licence required for all parties; restricted use</Availability>
                    <DistributionMedium>3 DVDs</DistributionMedium>
                    <CatalogueLink>http://www.inl.nl/en/corpora/autonomata-spoken-names-corpus</CatalogueLink>
                    <Contact>
                        <Email>servidesk@inl.nl</Email>
                        <Organisation>TST-Centrale/INL</Organisation>
                    </Contact>
                    <Price>
                        <Price>free for academic parties; 4000 euro for commercial parties (excl. VAT)</Price>
                    </Price>
                </Access>
                <CollectionType>
                    <CollectionType>audio</CollectionType>
                </CollectionType>
