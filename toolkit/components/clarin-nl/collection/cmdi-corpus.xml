<?xml version="1.0" encoding="UTF-8"?>
<CMD_ComponentSpec xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="file:../../general-component-schema.xsd">
    <Header></Header>
    <CMD_Component name="corpus">
        
        <CMD_Element name="multilinguality">
            <ValueScheme>
                <enumeration>
                    <item>monolingual</item>
                    <item>bilingual</item>
                    <item>multilingual</item>
                </enumeration>
            </ValueScheme>
        </CMD_Element>
        
        <!-- eg a corpus of travel planning prompts  -->
        <CMD_Element name="Topic" CardinalityMin="0" CardinalityMax="unbounded" ValueScheme="string"/>
        
        <!-- include INL collection components -->
        <CMD_Component filename="component-size.xml"></CMD_Component>
        <CMD_Component filename="component-project.xml"></CMD_Component>
        <CMD_Component filename="component-creators.xml"></CMD_Component>
        <CMD_Component filename="component-subjectlanguages.xml"></CMD_Component>
        <CMD_Component filename="component-documentlanguages.xml"></CMD_Component>
        <CMD_Component filename="component-access.xml"></CMD_Component>
                
    </CMD_Component>
</CMD_ComponentSpec>