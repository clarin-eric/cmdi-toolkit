<?xml version="1.0" encoding="UTF-8"?>
<CMD_ComponentSpec xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../general-component-schema.xsd" isProfile="false">
    <Header>
        <ID>eu.clarin.cmdi.webservice.operation</ID>
        <Name>Operation</Name>
        <Description>The Operation Component describes the metadata characteristics for an operation. Operation information, including input and output parameters, and  may initially be extracted from the WSDL or WADL file.</Description>
    </Header>
    
    <CMD_Component name="operation">
        <CMD_Element name="PersistentIdentifier" ValueScheme="anyURI" ConceptLink="http://www.isocat.org/datcat/DC-2573"/>
        <!-- missing datcat for name -->
        <CMD_Element name="Name" ValueScheme="string" />
        
        <CMD_Component filename="input.xml"/>
        <CMD_Component filename="output.xml"/>
        
    </CMD_Component>
</CMD_ComponentSpec>
