<?xml version="1.0" encoding="UTF-8"?>
<CMD_ComponentSpec xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.clarin.eu/cmd http://www.clarin.eu/cmd/general-component-schema.xsd" isProfile="false">
 <Header>
   <Description>The component TimePeriod describes if a resource is synchronic or diachronic</Description>
 </Header>
    <CMD_Component name="TimePeriod" CardinalityMin="0" CardinalityMax="unbounded">
        <CMD_Element name="TimePeriod" ConceptLink="http://www.isocat.org/datcat/DC-2502">
            <ValueScheme>
                <enumeration>
                    <item>Synchronic</item>
                    <item>Diachronic</item>
                </enumeration>
            </ValueScheme>
        </CMD_Element>  
    </CMD_Component>    
</CMD_ComponentSpec>
