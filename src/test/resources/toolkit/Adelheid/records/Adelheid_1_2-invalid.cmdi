                                <cmdp:TechnicalMetadata>
                                    <cmdp:CharacterEncoding>UTF-8</cmdp:CharacterEncoding>
                                </cmdp:TechnicalMetadata>
                            </cmdp:Parameter>
                        </cmdp:Input>
                        <cmdp:Output>
                            <cmdp:ParameterGroup>
                                <cmdp:Name>status</cmdp:Name>
                                <cmdp:MIMEType>text/xml</cmdp:MIMEType>
                                <cmdp:Parameters>
                                    <cmdp:Parameter>
                                        <cmdp:Name>project</cmdp:Name>
                                        <cmdp:Description>The name of the new Adelheid project.</cmdp:Description>
                                        <cmdp:DataType>string</cmdp:DataType>
                                        <cmdp:SemanticType>clam.project.adelheid</cmdp:SemanticType>
                                        <cmdp:TechnicalMetadata>
                                            <cmdp:ContentEncoding>
