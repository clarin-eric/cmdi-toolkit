<?xml version="1.0" encoding="UTF-8"?>
<CMD_ComponentSpec xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.clarin.eu/cmd http://www.clarin.eu/cmd/general-component-schema.xsd"
                   isProfile="false">
    <Header>
         <Name>Duration</Name>
               <Description>Component contains information about the start and/or completion year of e.g. a project </Description>
    </Header>
     
    <CMD_Component name="Duration">
        <CMD_Element name="StartYear" ValueScheme="string"
                   ConceptLink="http://www.isocat.org/datcat/DC-2539"
                   CardinalityMin="0"
                   CardinalityMax="1"> </CMD_Element>
        <CMD_Element name="CompletionYear" ValueScheme="string"
                   ConceptLink="http://www.isocat.org/datcat/DC-2509"
                   CardinalityMin="0"
                   CardinalityMax="1"> </CMD_Element>
    </CMD_Component>
</CMD_ComponentSpec>