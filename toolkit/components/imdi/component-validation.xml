<?xml version="1.0" encoding="UTF-8"?>

<CMD_ComponentSpec xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../general-component-schema.xsd">
    
    <Header/>
    
    <CMD_Component name="Validation" CardinalityMin="1" CardinalityMax="1">
        <CMD_Element name="Type">
            <ValueScheme>
                <enumeration>
                    <item>Unknown</item>
                    <item>Unspecified</item>
                    <item>None</item>
                    <item>Formal</item>
                    <item>Content</item>
                    <item>Formal/Content</item>
                </enumeration>
            </ValueScheme>
        </CMD_Element>
        <CMD_Element name="Methodology">
            <ValueScheme>
                <enumeration>
                    <item>Unknown</item>
                    <item>Unspecified</item>
                    <item>Hand</item>
                    <item>Automatic</item>
                    <item>Semi-Automatic</item>
                </enumeration>                
            </ValueScheme>
        </CMD_Element>
        <CMD_Element name="Level" ValueScheme="string"/>
        <CMD_Component filename="component-description.xml" />
    </CMD_Component>

</CMD_ComponentSpec>
