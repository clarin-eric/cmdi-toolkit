    xsi:schemaLocation="http://www.clarin.eu/cmd http://www.clarin.eu/cmd/general-component-schema.xsd" isProfile="false">
    
    <Header>
        <Name>Contact</Name>
        <Description>Component contains contact information about creators, distributors, actors,... of a resource </Description>
