    xsi:schemaLocation="http://www.clarin.eu/cmd http://www.clarin.eu/cmd/general-component-schema.xsd" isProfile="false">
    <Header>
        <ID></ID>
        <Name>Text Corpus</Name>
        <Description>A component for metadata which is characteristic for a text corpus</Description>
    </Header>
    
    <CMD_Component name="TextCorpus" CardinalityMax="1">
              
        <CMD_Element name="SourceType" ConceptLink="http://www.isocat.org/datcat/DC-2990" ValueScheme="string" CardinalityMin="0" CardinalityMax="unbounded"/>   

        <!-- the component TextTechnical contains technical information about the text corpus, like the mime type and the character encoding -->
        <CMD_Component filename="http://www.clarin.eu/cmd/components/clarin-nl/collection/cmdi-text-technical.xml" CardinalityMin="0" CardinalityMax="1"/>
        
    </CMD_Component>

